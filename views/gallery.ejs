        <section id="gallery">
          <div class="container">
            <div class="caption-container">
              <p id="caption"></p>
            </div>
            <div class="mySlides">
              <img src="/static/images/beforeAfter/108livingroomBEFORE.jpg"
              alt="108th Living room before" style="width:100%">
            </div>
            <div class="mySlides">
              <img src="/static/images/beforeAfter/108livingroomAFTER.jpg" alt="108th Living room after" style="width:100%">
